{
  "name": "router5-history",
  "version": "1.0.1",
  "description": "History plugin for router5",
  "main": "dist/commonjs/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && mkdirp dist/commonjs && npm run build:cjs && npm run build:iife && npm run build:amd && npm run build:umd",
    "build:iife": "rollup -c -f iife && rollup -c -f iife --uglify",
    "build:amd": "rollup -c -f amd && rollup -c -f amd --uglify",
    "build:umd": "rollup -c -f umd && rollup -c -f umd --uglify",
    "build:cjs": "babel modules --out-dir dist/commonjs",
    "test": "karma start karma.config.js",
    "clog": "conventional-changelog -p angular -i CHANGELOG.md -w",
    "lint": "eslint modules",
    "release": "./scripts/release.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/router5/router5-history.git"
  },
  "keywords": [
    "router5",
    "plugin",
    "history",
    "html5",
    "router"
  ],
  "author": "Thomas Roch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/router5/router5-history/issues"
  },
  "homepage": "http://router5.github.io",
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-export-extensions": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-rollup": "^1.0.0",
    "conventional-changelog": "^0.5.3",
    "coveralls": "^2.11.4",
    "eslint": "^1.10.3",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.11",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.2",
    "karma-coveralls": "^1.1.2",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jasmine": "^0.3.6",
    "karma-mocha-reporter": "^1.1.1",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.0",
    "rollup": "^0.24.0",
    "rollup-plugin-babel": "^2.3.6",
    "rollup-plugin-npm": "^1.2.0",
    "rollup-plugin-uglify": "^0.1.0",
    "router5": "router5/router5#v2",
    "should": "^7.1.0",
    "yargs": "^3.27.0"
  }
}
